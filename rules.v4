#/etc/iptables/rules.v4

*filter
:INPUT ACCEPT [1019:107717]
:FORWARD ACCEPT [0:0]
:OUTPUT ACCEPT [720:92470]
-A FORWARD -m conntrack --ctstate RELATED,ESTABLISHED -j ACCEPT
-A FORWARD -i wlan0 -o wlan1 -j ACCEPT
COMMIT

*nat
:PREROUTING ACCEPT [199:67406]
:INPUT ACCEPT [42:6723]
:OUTPUT ACCEPT [21:2136]
:POSTROUTING ACCEPT [4:944]
-A POSTROUTING -o wlan1 -j MASQUERADE
COMMIT